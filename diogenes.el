(defun diogenes-delete-line-numbers ()
  "Delete line numbers, starting at point"
  (interactive)
  (save-excursion 
    (let ((start (progn
		   (unless
		       (re-search-backward
			"\([[:digit:]][[:digit:]][[:digit:]][[:digit:]]: [[:digit:]][[:digit:]][[:digit:]]\)"
			nil t)
		     (re-search-forward 
			"\([[:digit:]][[:digit:]][[:digit:]][[:digit:]]: [[:digit:]][[:digit:]][[:digit:]]\)"))
		   (re-search-forward "^$")
		   (point)))
	  (end (progn
		 ;; (goto-char (point-max))
		 (unless
		     (re-search-forward "diogenes-browse finished" nil t)
		   (goto-char (point-max)))
		 (re-search-backward "[Α‐ω]")
		 (beginning-of-line)
		 (forward-char 14)
		 (point))))
      (delete-rectangle start end))))

(defun diogenes-tidy-up-browser-output-greek ()
  "Tidy up the output of the Diogenes Browser.
Works on the rest of the buffer starting at point, or on the
active region."
  (interactive)
  ;; Delete the header
  (re-search-backward "^[[:digit:]][[:digit:]][[:digit:]][[:digit:]]: [<\[]?[A-Z]")
  (save-excursion
    (save-restriction
      (narrow-to-region (point) (point-max))
      (re-search-forward "\([[:digit:]][[:digit:]][[:digit:]][[:digit:]]: [[:digit:]][[:digit:]][[:digit:]]\)")
      (previous-line)		 
      ;; (re-search-forward "line\\?")
      ;; (next-line)
      (delete-backward-char (- (point) (point-min)))
      (delete-blank-lines)
      ;; Deleting the queries in the main text
      (while (re-search-forward "\\[more\\], \\[b\\]ack, \\[q\\]uit?" (point-max) t)
	(move-beginning-of-line nil)
	(previous-line)
	(let ((delete-start (point))
	      (delete-end (progn (next-line 6)
				 (point))))
	  (delete-region delete-start delete-end))
	(delete-blank-lines)
	(delete-blank-lines)
	;; Delete surplus line numbers
	(re-search-forward "[[:space:]]")
	(re-search-backward "[[:alnum:]]")
	(unless (or
		 (= ?0 (char-after))
		 (= ?1 (char-after))
		 (= ?5 (char-after)))
	  (let ((strlength (current-column)))
	    (delete-char 1)
	    (delete-backward-char strlength)
	    (insert-char 32 (+ 1 strlength)))))
      (goto-char (point-max))
      (insert-char ?% 70)
      (newline))))

