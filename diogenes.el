(defun diogenes-ol-to-ad (ol)
  "Converts Ol. to A.D."
  (interactive "nPlease enter the Olypiad: ")
  (let ((ad (+ -776 (* 4 (- ol 1)))))
    (if (< ad 0)
	(message "%d BC" (- 0 ad))
      (message "%d AD" (- ad 1)))))

(defun diogenes-ad-to-ol (ad)
  "Converts A.D. to Ol."
  (interactive "nPlease enter a year: ")
  (let ((ol)(yr))
    (when (< ad 0)
      (setq ol (+ ad 780))
      (setq yr (+ 1(% ol 4)))
      (setq ol (/ ol 4))
      (message "Ol. %d.%d" ol yr))
    (when (= ad 0)
      (error "There is no year 0!"))
    (when (> ad 0)
      (setq ol (+ ad 779))
      (setq yr (+ 1(% ol 4)))
      (setq ol (/ ol 4))
	(message "Ol. %d.%d" ol yr))))

(defun diogenes-browser-insert-line-numbers ()
  "Insert missing line numbers in diogenes browser output"
  (interactive)
  (while 
      (re-search-forward "^\\S-*[0-9]+")
    (let ((counter (string-to-number (thing-at-point 'word t))))
    (while
	(progn
	  (re-search-forward "^")
	  (string-match "\s" (char-to-string (char-after))))
      (insert (number-to-string (setq counter (1+ counter))))
      (forward-word)))))


